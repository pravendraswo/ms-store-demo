<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Azure blue gradient background -->
  <defs>
    <radialGradient id="azureGrad" cx="50%" cy="30%" r="70%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0078D4;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Cloud shape -->
  <ellipse cx="150" cy="200" rx="120" ry="80" fill="url(#azureGrad)" stroke="#005A9E" stroke-width="2"/>
  <ellipse cx="100" cy="170" rx="60" ry="50" fill="url(#azureGrad)" stroke="#005A9E" stroke-width="2"/>
  <ellipse cx="200" cy="170" rx="60" ry="50" fill="url(#azureGrad)" stroke="#005A9E" stroke-width="2"/>
  <ellipse cx="150" cy="140" rx="80" ry="60" fill="url(#azureGrad)" stroke="#005A9E" stroke-width="2"/>
  
  <!-- Azure logo style -->
  <rect x="130" y="120" width="40" height="15" fill="white" opacity="0.9"/>
  <text x="150" y="132" font-family="Arial, sans-serif" font-size="8" text-anchor="middle" fill="#0078D4" font-weight="bold">AZURE</text>
  
  <!-- Cute face -->
  <circle cx="125" cy="180" r="8" fill="white"/>
  <circle cx="175" cy="180" r="8" fill="white"/>
  <circle cx="125" cy="180" r="4" fill="#333"/>
  <circle cx="175" cy="180" r="4" fill="#333"/>
  <path d="M 135 200 Q 150 210 165 200" stroke="#333" stroke-width="2" fill="none"/>
  
  <!-- Plushie texture lines -->
  <path d="M 80 160 Q 150 150 220 160" stroke="#005A9E" stroke-width="1" opacity="0.3" fill="none"/>
  <path d="M 90 190 Q 150 180 210 190" stroke="#005A9E" stroke-width="1" opacity="0.3" fill="none"/>
  
  <text x="150" y="280" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#0078D4" font-weight="bold">Azure Cloud Plushie</text>
</svg>