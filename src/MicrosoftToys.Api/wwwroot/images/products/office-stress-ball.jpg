<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Office orange gradient -->
  <defs>
    <radialGradient id="officeGrad" cx="40%" cy="40%" r="60%">
      <stop offset="0%" style="stop-color:#FF8C00;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D2691E;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Shadow -->
  <ellipse cx="155" cy="260" rx="60" ry="15" fill="#000" opacity="0.2"/>
  
  <!-- Main stress ball -->
  <circle cx="150" cy="150" r="80" fill="url(#officeGrad)" stroke="#CD853F" stroke-width="2"/>
  
  <!-- Office 365 logo background -->
  <rect x="120" y="120" width="60" height="60" fill="white" opacity="0.9" rx="5"/>
  
  <!-- Office 365 icon grid -->
  <rect x="130" y="130" width="15" height="15" fill="#D83B01" rx="2"/>
  <rect x="150" y="130" width="15" height="15" fill="#0078D4" rx="2"/>
  <rect x="130" y="150" width="15" height="15" fill="#107C10" rx="2"/>
  <rect x="150" y="150" width="15" height="15" fill="#FFB900" rx="2"/>
  
  <!-- 365 text -->
  <text x="150" y="175" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="#333" font-weight="bold">365</text>
  
  <!-- Stress ball texture -->
  <circle cx="150" cy="150" r="75" fill="none" stroke="white" stroke-width="1" opacity="0.3"/>
  <circle cx="150" cy="150" r="65" fill="none" stroke="white" stroke-width="1" opacity="0.2"/>
  <circle cx="150" cy="150" r="55" fill="none" stroke="white" stroke-width="1" opacity="0.1"/>
  
  <!-- Squeeze marks -->
  <path d="M 90 120 Q 150 115 210 120" stroke="#CD853F" stroke-width="2" opacity="0.5" fill="none"/>
  <path d="M 90 180 Q 150 185 210 180" stroke="#CD853F" stroke-width="2" opacity="0.5" fill="none"/>
  <path d="M 120 90 Q 125 150 120 210" stroke="#CD853F" stroke-width="2" opacity="0.5" fill="none"/>
  <path d="M 180 90 Q 175 150 180 210" stroke="#CD853F" stroke-width="2" opacity="0.5" fill="none"/>
  
  <text x="150" y="280" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#D2691E" font-weight="bold">Office 365 Stress Ball</text>
</svg>